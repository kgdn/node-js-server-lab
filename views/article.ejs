<%- include('header'); -%>
<div class="content">
  <h1>Selected articles</h1>
  <table id="catalog">
    <thead>
      <tr>
        <th scope="col">Reference No.</th>
        <th scope="col">Category</th>
        <th scope="col">Name</th>
        <th scope="col">Supplier</th>
        <th scope="col">Price</th>
        <th scope="col">VAT</th>
        <th scope="col">Stock</th>
        <th scope="col">Critical Stock</th>
      </tr>
    </thead>
    <tbody>
      <% { %>
      <tr>
        <td><%= product.reference %></td>
        <td><%= product.category %></td>
        <td><%= product.name %></td>
        <td><%= product.supplier %></td>
        <td><%= product.price %>AED</td>
        <td><%= product.vat %></td>
        <td><%= product.stock %></td>
        <td><%= product.critical_stock %></td>
      </tr>
      <% } %>
    </tbody>
  </table>
</div>
<br />
<%- include('footer'); -%>
